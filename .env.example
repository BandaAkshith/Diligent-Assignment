PINECONE_API_KEY=your_key
PINECONE_INDEX=diligent
PINECONE_CLOUD=aws
PINECONE_REGION=us-east-1
# Optional: if using a direct host for your index
# PINECONE_HOST=

EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIM=384

# Ollama model name (pulled via `ollama run ...`)
OLLAMA_MODEL=llama3.1
LLM_CTX=4096
LLM_THREADS=6
LLM_TEMPERATURE=0.3
LLM_MAX_TOKENS=512
